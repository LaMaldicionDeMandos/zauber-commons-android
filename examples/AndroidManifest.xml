<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.zauberlabs.android.examples" android:versionCode="1" android:versionName="1.0.0-SNAPSHOT">

  <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="15" />
  <uses-permission android:name="android.permission.INTERNET"/>


  <application android:icon="@drawable/icon" android:label="@string/app_name">
    <activity android:name=".MainActivity" android:screenOrientation="portrait">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".PaginatorActivity" android:screenOrientation="portrait" />
    <activity android:name=".VerticalScrollActivity" android:screenOrientation="portrait" />
    <activity android:name=".OldVerticalScrollActivity" android:screenOrientation="portrait" />
  </application>

</manifest>

